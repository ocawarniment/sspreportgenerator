{
  "manifest_version": 3,
  "name": "OCA: SSP Report Generator",
  "version": "25.0.0",
  "description": "OCA extension for the counseling team to prepare SSP reports.",
  "action": {
    "default_icon": "/images/icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "webNavigation",
    "storage",
    "tabs",
    "scripting"
  ],
  "host_permissions": [
    "<all_urls>"
  ]
}